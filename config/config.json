{
  "targets": [
    {
      "modules": ["http", "dns", "tls"],
      "resources": ["https://example.com"]
    },
    {
      "modules": ["simplechecksums"],
      "resources": ["https://github.com/gohugoio/hugo/releases/download/v0.155.2/hugo_0.155.2_checksums.txt"]
    }
  ],
  "modules": {
    "dns": {
      "command": "python3 ./modules/moddns.py"
    },
    "simplechecksums": {
      "command": "python3 ./modules/modsimplechecksums.py"
    },
    "http": {
      "command": "python3 ./modules/modhttp.py"
    },
    "tls": {
      "command": "python3 ./modules/modtls.py"
    },
    "github": {
      "command": "python3 ./modules/modgithub.py"
    }
  }
}
